{
  "name": "@zuclubit/lead-service",
  "version": "0.1.0",
  "private": true,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup src/app.ts --format cjs --dts",
    "dev": "tsx watch src/app.ts",
    "start": "node dist/app.js",
    "clean": "rm -rf dist",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "db:generate": "drizzle-kit generate:pg",
    "db:migrate": "drizzle-kit push:pg",
    "db:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@fastify/compress": "^8.3.0",
    "@fastify/cors": "^11.1.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/rate-limit": "^10.3.0",
    "@fastify/swagger": "^9.6.1",
    "@fastify/swagger-ui": "^5.2.3",
    "@zuclubit/database": "*",
    "@zuclubit/domain": "*",
    "@zuclubit/events": "*",
    "dotenv": "^16.3.1",
    "fastify": "^4.29.1",
    "fastify-zod": "^1.4.0",
    "reflect-metadata": "^0.2.1",
    "tsyringe": "^4.8.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@testcontainers/postgresql": "^11.8.1",
    "@types/node": "^20.11.0",
    "@types/supertest": "^6.0.2",
    "@vitest/coverage-v8": "^1.2.0",
    "drizzle-kit": "^0.20.10",
    "pino-pretty": "^13.1.2",
    "supertest": "^6.3.4",
    "testcontainers": "^10.5.0",
    "tsup": "^8.0.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  }
}
