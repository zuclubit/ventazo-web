  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "build:cf": "npx opennextjs-cloudflare build && npm run postbuild:cf",
    "postbuild:cf": "cd .open-next && cp -r assets/_next . && cp -r assets/images . && cp assets/favicon* . 2>/dev/null; cp assets/android-chrome* . 2>/dev/null; cp assets/apple-touch-icon.png . 2>/dev/null; cp assets/robots.txt . 2>/dev/null; cp assets/site.webmanifest . 2>/dev/null; cp ../public/_headers . 2>/dev/null; [ -f worker.js ] && mv worker.js _worker.js || true; cd .. && node scripts/generate-routes.mjs",
    "postbuild:generate-routes": "node scripts/generate-routes.mjs",
    "start": "next start",
    "preview": "npx wrangler dev",
    "deploy": "npx wrangler deploy",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "clean": "rm -rf .next .turbo .vercel node_modules"
  },
